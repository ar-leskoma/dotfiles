# This is Git's per-user configuration file.
[user]
	name = Matthew Lesko-Krleza
	email = mlesko1996@gmail.com

[core]
	editor = /usr/bin/vim

[alias]
	s = status
	l = log
	d = diff
	ca = commit --amend
	ch = checkout
    c = checkout
    cm = commit 
    b = branch
    lg = log --color --graph --all --abbrev-commit --decorate --pretty=oneline

[pull]
    rebase = true
